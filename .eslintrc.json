{
    "extends": [
        "airbnb-base"
    ],
    "overrides": [
        {
            "files": [
                "test/**"
            ],
            "plugins": [
                "jest"
            ],
            "extends": [
                "plugin:jest/all"
            ],
            "rules": {
                "jest/no-hooks": "off"
            }
        }
    ],
    "rules": {
        "indent": [
            "error",
            4
        ],
        "no-console": "off"
    }
}
